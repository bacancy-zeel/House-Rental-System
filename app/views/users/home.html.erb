<div>
    <%= form_with model: @house, method: :get do |f| %>
        <div class="field">
        <!-- Country -->
        <%= f.label :state %>
        <%= f.select :state, options_for_select(CS.states(:in).map { |c| [c[1], c[0]] } ) %>
        </div>

        <br>

        <div class="field">
        <!-- State -->
        <%= f.label :city %>
        <%= f.select :city, options_for_select([CS.cities(:AN)] ) %>
        </div>
    <% end %>
    
</div>




