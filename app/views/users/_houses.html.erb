<table class="table">
  <thead>
    <tr>
      <th>Type</th>
      <th>Category</th>
      <th>Square feet</th>
      <th>Amount</th>
      <th>View Details</th>
    </tr>
  </thead>

  <tbody>

    <% @houses.each do |house| %>
      <tr>
      
        <td><%= house.house_address %></td>
        <td><%= house.city %></td>
        
        <td><%= link_to 'View Details', show_user_path(house) %></td>
    
      </tr>
       <% end %>

  
    
  </tbody>
</table>