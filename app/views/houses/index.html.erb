<%= link_to 'Post House', new_house_path %>


<h3>Houses Posted by <%= current_user.first_name %> <%= current_user.last_name %></h3>


<table border="1">
  <thead>
    <tr>
      <th>House Type</th>
      <th>Category</th>
      <th>Square Feet</th>
      <th>Amount</th>
    </tr>
  </thead>

  <tbody>
    <% @house.each do |house| %>
      <tr>
        <td><%= house.house_type %></td>
        <td><%= house.category %></td>
        <td><%= house.square_feet %></td>
        <td><%= house.amount %></td>
        <td><%= link_to 'Edit', edit_house_path(house) %></td>
        <td><%= link_to 'Destroy', house, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Back', root_path %>



