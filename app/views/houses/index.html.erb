
<%if current_user%>
    <%= link_to 'Post House', posts_path %>
<% else %>
    <%= link_to 'Post House', user_session_path %>
<% end %>
<br>
<div>
    <%= form_with model: @house, method: :get, local: true do |f| %>
 <div class="field">
  <!-- Country -->
  <%= f.label :state %>
  <%= f.select :state, options_for_select(CS.states(:in).map { |c| [c[1], c[0]] } ) %>
</div>

<div class="field">
  <!-- State -->
  <%= f.label :city %>
  <%= f.select :city, options_for_select([]) %>
</div>


    
    <% end %>
</div>



