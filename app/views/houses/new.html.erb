
<%= form_with(model: @house, local: true) do |form| %> 

    <%= form.label :House_Type %>
    <%= form.select :house_type, options_for_select([['Apartments'],['Villa/House'],['Flats'],['Row House'],['Farm House']]) %><br><br>

    <%= form.label :Category %>
    <%= form.select :category, options_for_select(House::Categories) %><br><br>

    <%= form.label :Square_Feet %>
    <%= form.text_field :square_feet %><br><br>

    <%= form.label :Amount %>
    <%= form.text_field :amount %><br><br>

    <%= form.label :House_Image %>
    <%= form.file_field :images, multiple: true %><br><br>

    <%= form.submit :Add_Address %>

<% end %>         
<br>
<%= link_to 'Back', houses_path %>